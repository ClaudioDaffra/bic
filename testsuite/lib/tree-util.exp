proc re_tree_preamble { type } {
    return "<tree at 0x\[0-9a-f]+, next \(\\(nil\\)|0x\[0-9a-f\]+\), type $type"
}

proc re_tree_integer { n } {
    set RE [re_tree_preamble "T_INTEGER"]
    set RE "$RE, number $n>"
    return $RE
}

proc expect_tree_integer { n } {
    global test
     expect {
         -re [re_tree_integer $n] { pass "$test" }
    }
}
